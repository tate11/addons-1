<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="profiler_res_partner_form_view" model="ir.ui.view">
        <field name="inherit_id" ref="base.view_partner_form"/>
        <field name="model">res.partner</field>
        <field name="arch" type="xml">
            <xpath expr="//sheet" position="before">
                <header>
                    <button name="find_duplicate_contacts_bad_perf" type="object" string="Profile bad function"/>
                    <button name="find_duplicate_contacts_good_perf" type="object" string="Profile good function"/>
                </header>
            </xpath>
        </field>
    </record>
</odoo>
